<struts>
    <package name="gii-page-default" namespace="/" extends="struts-default">
        <!-- 首页 -->
        <action name="gii" class="org.warnier.zhang.gii.action.GiiAction">
            <result name="indexPage" type="freemarker">WEB-INF/index.ftl</result>
